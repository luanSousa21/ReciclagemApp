<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.reciclandoapp.ui.inicio.InicioFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_inicio" />

    <fragment
        android:id="@+id/navigation_add"
        android:name="com.example.reciclandoapp.ui.adicionar.AdicionarFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_adicionar" />
    <fragment
        android:id="@+id/navigation_perfil"
        android:name="com.example.reciclandoapp.ui.perfil.PerfilFragment"
        android:label="@string/title_perfil"
        tools:layout="@layout/perfil_fragment" />
</navigation>